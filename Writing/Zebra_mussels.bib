issn = {0740-2783},
journal = {American malacological bulletin},
keywords = {Dreissena polymorpha,moule,mussel},
number = {112},
pages = {141--145},
title = {{Recent population changes in freshwater mussels (Bivalvia: Unionidae) and zebra mussels (Dreissena polymorpha) in Lake St. Clair, U.S.A.}},
volume = {Vol. 16, N},
year = {2001}

@article{Bravington2018,
abstract = {Density Surface Models (DSMs) are two-stage models for estimating animal density from line-transect data. First, detectability is estimated by distance sampling, and then a Generalized Additive Model (GAM) is used to estimate animal density as a function of location and/or other spatially-varying environmental covariates. One criticism of DSMs has been that uncertainty from the two stages is not usually propagated correctly into the final variance estimates. Here we show how to reformulate a DSM so that the uncertainty in detection probability from the distance sampling stage (regardless of its complexity) is captured as a random effect in the GAM stage. This allows straightforward computation of the overall variance via standard GAM machinery. We further extend the formulation to allow for spatial variation in group size, which can be an important covariate for detectability. We illustrate these models using line transect survey data of minke whales and harbour porpoise from the SCANS-II survey in northern Europe.},
archivePrefix = {arXiv},
arxivId = {1807.07996},
author = {Bravington, Mark V and Miller, David L and Hedley, Sharon L},
eprint = {1807.07996},
file = {:home/troutinthemilk/Dropbox/LIT/Bravington etal 2018.pdf:pdf},
keywords = {abundance estimation,distance sampling,generalized additive models,group size,line transect sampling,spatial modelling,wildlife surveys},
pages = {1--20},
title = {{Reliable variance propagation for spatial density surface models}},
url = {http://arxiv.org/abs/1807.07996},
year = {2018}
}
@article{Drake2004b,
abstract = {The range expansion of zebra mussels (Dreissena polymorpha) in North America has been rapid and costly in both economic and ecological terms. joint social, political, and scientific ventures such as the 100th Meridian Initiative aim to reduce the spread of zebra mussels by eliminating the unintended transport of the species and preventing its westward expansion. Here we forecast the potential distribution of zebra mussels in the United States by applying a machine-learning algorithm for nonparametric prediction of species distributions (genetic algorithm for rule-set production, or GARP) to data about the current distribution of zebra mussels in the United States and 11 environmental and geological covariates. Our results suggest that much of the American West will be uninhabitable for zebra mussels. Nonetheless, some catchments along the West Coast and in the southeastern United States exhibit considerable risk of invasion and should be monitored carefully. Possible propagule dispersal to these places should be managed proactively.},

isbn = {0004-8038},
issn = {0004-8038},
journal = {The Auk},
keywords = {abundance estimation,bird surveys,capture-recapture,detection,en puntos de conteo,estimacion de la abundancia,mediante el metodo del,point counts,probability,tiempo},
number = {2},

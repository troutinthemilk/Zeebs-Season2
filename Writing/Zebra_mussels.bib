file = {:home/troutinthemilk/Dropbox/LIT/Burt{\_}et{\_}al-2014-Methods{\_}in{\_}Ecology{\_}and{\_}Evolution.pdf:pdf},
issn = {2041210X},
journal = {Methods in Ecology and Evolution},
keywords = {Distance sampling,Double-observer survey,Line transects,Mark-recapture,Program distance},
number = {11},

isbn = {00129658},
issn = {00129658},
journal = {Ecology},
keywords = {Dreissena polymorpha,Exotic,Extinction,Gravity model,Incidence model,Invasive species,Nonindigenous,Risk,Time series,Zebra mussel},
number = {6},
pages = {1651--1660},
